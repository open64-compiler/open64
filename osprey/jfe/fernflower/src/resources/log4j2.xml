<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
       <Console name="Console" target="SYSTEM_OUT">
          <PatternLayout pattern="%d{HH:mm:ss} %-5level %class{36} %L %M - %msg%xEx%n"/>
       </Console>
    </Appenders>
    <Loggers>
       <Root level="All">
          <AppenderRef ref="Console"/>
       </Root>
    </Loggers>
</Configuration>
